Host *
	ServerAliveInterval 300
	ServerAliveCountMax 2

Host fjlaptop-worker-1
HostName 192.168.56.69
User qserv
Port 22
StrictHostKeyChecking no
UserKnownHostsFile /dev/null
PasswordAuthentication no
ProxyCommand ssh -i ~/.ssh/id_rsa_openstack -q -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -W %h:%p qserv@193.55.95.175
IdentityFile ~/.ssh/id_rsa_openstack
IdentitiesOnly yes
LogLevel FATAL


